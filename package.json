{
  "name": "postcss-remove-font-face-format",
  "version": "0.0.0",
  "description": "PostCSS plugin to remove font faces by format",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-remove-font-face-format"
  ],
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "author": "Swiss GRC AG <opensource@swissgrc.com>",
  "license": "MIT",
  "repository": "swissgrc/postcss-remove-font-face-format",
  "engines": {
    "node": ">=12.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.3.0"
  },
  "devDependencies": {
    "clean-publish": "^4.0.0",
    "eslint": "^8.12.0",
    "eslint-plugin-jest": "^26.1.3",
    "jest": "^27.3.1",
    "postcss": "^8.4.12"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "dependencies": {
    "postcss-value-parser": "^4.2.0"
  }
}
