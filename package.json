{
  "name": "postcss-remove-font-face-format",
  "version": "0.0.0",
  "description": "PostCSS plugin to remove font faces by format",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-remove-font-face-format"
  ],
  "scripts": {
    "test": "jest --coverage && eslint .",
    "prepack": "clean-package",
    "new:pack": "yarn pack && clean-package restore",
    "new:publish": "yarn publish && clean-package restore"
  },
  "author": "Swiss GRC AG <opensource@swissgrc.com>",
  "license": "MIT",
  "repository": "swissgrc/postcss-remove-font-face-format",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "postcss": "8.4.0"
  },
  "devDependencies": {
    "clean-package": "2.1.1",
    "eslint": "9.0.0",
    "eslint-plugin-jest": "28.0.0",
    "jest": "29.3.1",
    "postcss": "8.4.12"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "scripts",
      "devDependencies",
      "eslintConfig",
      "jest",
      "clean-package"
    ]
  },
  "dependencies": {
    "postcss-value-parser": "4.2.0"
  }
}
